use XL.UI.CONSOLE

procedure Foo (X : integer) is
    WriteLn "Foo ", X

procedure Foo (X : real) is
    WriteLn "Foo (real) ", X

procedure Bar (X : integer) is
    WriteLn "Bar ", X

type proc_ptr is procedure (X : integer)

to Invoke (callback : proc_ptr) is
   callback 3

Invoke Foo
Invoke Bar


