// Check that we can instantiate with multiple parameters
// RUN=cat %c
generic type ordered

function F(A : ordered; B : ordered) return ordered is
    return A + B

Q : integer := F(1, 5)

function G(A, B : ordered; C : ordered; D : ordered) return ordered is
    return A + B * C/D

Z : integer := G(1, -3, 6,4)

