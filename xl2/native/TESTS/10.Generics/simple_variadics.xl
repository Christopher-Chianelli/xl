// Test that simple recursive definitions of variadics work
// RUN=cat %b.cpp

function Max (X : integer) return integer is
    return X

function Max (X : integer; ...) return integer is
    result := Max(...)
    if result < X then
        result := X

K : integer := Max(1, 2, 3, 4)
L : integer := Max(1, 2, 3)
