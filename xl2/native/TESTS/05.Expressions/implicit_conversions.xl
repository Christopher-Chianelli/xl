import CS = XL.UI.CONSOLE

function ConvertReal (X : integer) return real written X is
    CS.WriteLn "Convert ", X, " to real"
    return 0.09

function ConvertBoolean (X : integer) return boolean written X is
    CS.WriteLn "Convert ", X, " to boolean"
    return X <> 0

K : boolean := 1

Copy K, 2

L : real := 3

Copy L, 4
