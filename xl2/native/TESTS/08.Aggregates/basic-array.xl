use XL.ARRAY.BASIC
use XL.UI.CONSOLE

MyArray : array[5] of integer

to Dump(A : array[5] of integer) is
    for I in 0..4 loop
        WriteLn "A[", I, "]=", A[I]

for I in 0..4 loop
    MyArray[I] := I
Dump MyArray

Copy MyArray[0], 3
MyArray[1] := 3
MyArray[2] += 5

WriteLn "Test results: ", MyArray[0], ", ", MyArray[1], ", ", MyArray[2]

Dump MyArray
