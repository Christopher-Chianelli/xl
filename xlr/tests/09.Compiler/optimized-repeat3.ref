define internal i1 @xl_eval_repeat3(%closure*) {
allocas:
  %1 = getelementptr %closure* %0, i32 0, i32 0
  %2 = load i1 (%closure*)** %1
  %3 = getelementptr %closure* %0, i32 0, i32 0
  %4 = getelementptr %closure* %0, i32 0, i32 0
  %5 = call i1 %2(%closure* %0)
  store i1 (%closure*)* %2, i1 (%closure*)** %1
  %6 = call i1 %2(%closure* %0)
  store i1 (%closure*)* %2, i1 (%closure*)** %3
  %7 = call i1 %2(%closure* %0)
  store i1 (%closure*)* %2, i1 (%closure*)** %4
  ret i1 %7
}

define %Tree* @xl_program() {
allocas:
  %0 = alloca %closure
  %1 = call i1 @xl_eval_write_with_cr(i8* getelementptr inbounds ([12 x i8]* @text1, i32 0, i32 0))
  %2 = getelementptr %closure* %0, i32 0, i32 0
  store i1 (%closure*)* @xl_closure, i1 (%closure*)** %2
  %3 = call i1 @xl_eval_repeat3(%closure* %0)
  br i1 %3, label %isTrue, label %isFalse

isTrue:                                           ; preds = %allocas
  br label %booleanBoxed

isFalse:                                          ; preds = %allocas
  br label %booleanBoxed

booleanBoxed:                                     ; preds = %isFalse, %isTrue
  %.0.in = phi %Tree** [ @true, %isTrue ], [ @false, %isFalse ]
  %.0 = load %Tree** %.0.in
  ret %Tree* %.0
}
