rectangle x, y, z, t -> writeln "Rectangle 1: ", x, ", ", y, ", ", z, ", ", t
rectangle x, y -> writeln "Rectangle 2: ", x, ", ", y

foo ->
    rectangle x, y, z, t -> writeln "Rectangle 3: ", x, ", ", y, ", ", z, ", ", t
    rectangle x, y -> writeln "Rectangle 4: ", x, ", ", y
    rectangle 1,2,3,4
    rectangle 5, 6

rectangle 7, 8, 9, 10
rectangle 11, 12
foo
