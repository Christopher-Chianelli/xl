local_form_handler ->
    evaluation_error X -> writeln "Called the local evaluation error"
    ratapouet 7

local_error_handler ->
    error Msg, Arg -> writeln "Called the local error handler with msg ", Msg
    ratapouet 7

local_form_handler
local_error_handler
