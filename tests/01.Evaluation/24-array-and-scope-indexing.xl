A is "Outer A"
C is "Outer C"

foo is
    A is "Inner A"
    B is "Inner B"

    "Outer A" is "Indexed by Outer A"

print "foo.A=", foo.A
print "foo.B=", foo.B
print "foo.C=", foo.C

print "foo[A]=", foo[A]
print "foo[B]=", foo[B]
print "foo[C]=", foo[C]

print "foo(A)=", foo(A)
print "foo(B)=", foo(B)
print "foo(C)=", foo(C)

print "foo A=", foo A
print "foo B=", foo B
print "foo C=", foo C
