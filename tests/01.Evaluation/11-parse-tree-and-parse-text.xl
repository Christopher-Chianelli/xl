write_line "Z+1=", quote(Z+1)
write_line "X+2*if=", quote(X+2*if X then Y else Z)

write_line "A+B*C-D*E+F=", parse "A+B*C-D*E+F"
write_line "A+B*C-D*E+=", parse "A+B*C-D*E+"
write_line "2__3=", parse "2__3"
write_line "16#FF+2#1001+8#7.001e-4=", parse "16#FF+2#1001+8#7.001e-4"
write_line "2#1001=", parse "2"

write_line "foo 3=", foo 3
write_line "foo 551.3=", foo 551.3
write_line "foo Z+1=", foo (Z+1)
write_line "bar Z+1=", bar (Z+1)
foo X is quote(X + 2 * {X+1})
bar X is quote(X + 2 * ({X}+1))
