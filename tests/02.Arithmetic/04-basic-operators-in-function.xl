run_test x,y is
    print x+y
    print x-y
    print x*y
    print x/y
    print x mod y
    print x rem y
    print x and y
    print x or y
    print x xor y
    if x > 0 and y > 0 then
        print x shl y
        print x ashr y
        print x lshr y
    print -x
    print not x
    print abs x
    print "---"

run_test 75,17
run_test 75,-17
run_test -75,-17
run_test -75,17
run_test 0,1
run_test 3,2
run_test 9,3
