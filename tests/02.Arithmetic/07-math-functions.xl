run_test x is
    print abs x
    print sqrt x
    // Avoid platform-dependent rounding errors for very small values
    print 0.25 + sin x
    print 0.25 + cos x
    print 0.25 + tan x
    print asin x
    print acos x
    print atan x
    print exp x
    print expm1 x
    print log x
    print log10 x
    print log2 x
    print log1p x
    print "---"

run_test 0.13331
run_test 1.3331
run_test pi
run_test 2.0*pi
